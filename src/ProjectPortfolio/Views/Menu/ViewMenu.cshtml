@model ProjectPortfolio.ViewModels.ViewMenuViewModel

<h1>@Model.Menu.Name</h1>

<ul>
    @foreach (var projectMenu in Model.Items)
    {
        <li>@projectMenu.Project.Name</li>

    }
</ul>
@if (Model.Items.Count == 0)
{
    <p><i>No Menu Items</i></p>
}
<p><a asp-controller="Menu" asp-action="AddItem" asp-route-id="@Model.Menu.ID">Add Item</a></p>